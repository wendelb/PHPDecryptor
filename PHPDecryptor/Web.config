<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.0" />
  </system.web>
  <system.webServer>
    <rewrite>
      <rules>
        <rule name="SpecificRewrite" stopProcessing="true">
          <match url="^studenten/info.php" />
          <action type="Rewrite" url="/webForm.aspx" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>
